.title {
  margin-top: 10rpx;
  background-color: #e3e3e3;
  height: 70rpx;
  line-height: 70rpx;
  border: 3rpx solid #d6d4d6;
  display: flex;
  justify-content: space-around;
}

.title-name {
  font-size: 28rpx;
  color: black;
}

.title-time {
  font-size: 28rpx;
  color: #696969;
}

.scaninfo {
  position: relative;
}

text {
  font-size: 30rpx;
}

image {
  height: 120rpx;
  width: 120rpx;
  border: 3rpx solid black;
}

.scantitle {
  position: absolute;
  left: 30rpx;
  top: 10rpx;
}

.scanimg {
  position: absolute;
  left: 30rpx;
  top: 70rpx;
}

.scancontent {
  position: absolute;
  left: 230rpx;
  top: 60rpx;
  font-size: 30rpx;
}

.scantime {
  height: 200rpx;
  border-bottom: 20rpx solid #e0dce0;
  border-top: 20rpx solid #e0dce0;
  position: relative;
  top: 300rpx;
}

.yuyue {
  position: absolute;
  left: 30rpx;
}

.yuyueitem1 {
  font-size: 30rpx;
  position: absolute;
  top:38rpx;
  left: 31rpx;
}

.yuyueitem2 {
  font-size: 30rpx;
  position: absolute;
  top: 130rpx;
  left: 31rpx;
}

.scanaccount {
  height: 150rpx;
  position: relative;
  top: 300rpx;
  border-bottom: 10rpx solid #e0dce0;
  line-height: 50rpx;
}

.amount {
  margin-top: 30rpx;
  display: flex;
  justify-content: space-between;
}

.money {
  margin-top: 10rpx;
  display: flex;
  justify-content: space-between;
}

button {
  position: absolute;
  bottom: 10rpx;
  left: 10rpx;
  background-color: #ff9800;
  width: 96%;
  color: white;
}
